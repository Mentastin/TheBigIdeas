% blackboard typefaces by Anthony Phan.
% file: mbbi10.mf (parameters' file for 10 points italic fonts)
% last modification: 25.10.2001.

if unknown mbbbase: input mbbbase fi

font_identifier:="mbbi"; font_size 10pt#;

slant:=0.25;
square_dots:=false;
variant_a:=false; variant_f:=true;
variant_g:=false; variant_y:=false;
old_digits:=true; square_hebrew:=false;
ab_up_factor:=0.05; ab_down_factor:=0.15; 
hmn_factor:=0.15; uy_factor:=0.15;

input mbblight;
input mbbdrivr;