% last modification: 25.10.2001.

if unknown mbbbase: input mbbbase fi

font_identifier:="mbbheb"; font_size 10pt#;

slant:=0;
square_dots:=false;
math_fitting:=false;
variant_a:=false; variant_f:=false;
variant_g:=false; variant_y:=false;
old_digits:=true; square_hebrew:=false;
ab_up_factor:=0; ab_down_factor:=0; 
hmn_factor:=0; uy_factor:=0;

input mbblight;
input mbbhebdr;








