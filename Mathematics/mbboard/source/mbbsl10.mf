% blackboard typefaces by Anthony Phan.
% file: mbbsl10.mf (parameters' file for 10 points slanted fonts)
% last modification: 25.10.2001.

if unknown mbbbase: input mbbbase fi

font_identifier:="mbbsl"; font_size 10pt#;

slant:=1/6;
square_dots:=false;
variant_a:=true; variant_f:=false;
variant_g:=false; variant_y:=false;
old_digits:=false; square_hebrew:=false;
ab_up_factor:=0.05; ab_down_factor:=0.15; 
hmn_factor:=0.15; uy_factor:=0.15;

input mbblight;
input mbbdrivr;